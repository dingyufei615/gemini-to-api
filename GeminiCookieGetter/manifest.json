{
  "manifest_version": 3,
  "name": "Gemini Cookie 发送器",
  "version": "1.1",
  "description": "获取并发送 gemini.google.com 的指定 cookies 到特定URL。",
  "permissions": [
    "cookies"
  ],
  "host_permissions": [
    "https://gemini.google.com/",
    "http://192.168.202.190/"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}